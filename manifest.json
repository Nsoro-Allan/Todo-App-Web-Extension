{
    "manifest_version": 2,
    "name": "AC - Todo App",
    "version": "1.0",
    "description": "A simple todo app extension",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": [
        "storage"
    ]
}